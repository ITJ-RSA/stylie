<form action="">
  <label class="$millisecondInputContainer millisecond-input-container">
    <span class="$millisecondDisplay millisecond-display">
      {{millisecond}}
    </span>
    <input
      type="number"
      name="millisecond"
      class="$millisecond millisecond"
      value="{{millisecond}}"
    >
    <span>ms</span>
  </label>
  {{#properties}}
  <div class="property">
    <label title="{{name}}">
      <span>{{displayName}}</span>
      <input
        type="number"
        class="${{name}}"
        name="{{name}}"
        value="{{value}}"
      >
    </label>
    {{#canChangeEasingCurve}}
    <select
      class="${{name}}Select curve"
      data-property="{{name}}"
      title="Easing curve"
    >
      {{#curves}}
      <option>{{.}}</option>
      {{/curves}}
    </select>
    {{/canChangeEasingCurve}}
  </div>
  {{/properties}}
</form>
